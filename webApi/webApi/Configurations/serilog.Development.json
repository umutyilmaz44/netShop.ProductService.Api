{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.Graylog"],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {      
        "Name": "Console"
      },
      {
        "Name": "Graylog",
        "Args":{
          "hostnameOrAddress":"127.0.0.1",
          "port":"12201",
          "transportType":"Udp",
          "facility":"ProductService"
        }
      }
    ],

    "Properties": {
      "Application": "ProductService"
    }
  }
}
